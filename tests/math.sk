local function test():
	run_test("lerp_mid", lerp(1, 2), 1.5)
	run_test("lerp_mid_large", lerp(-200000, 200000), 0)
	run_test("lerp", lerp(0, 1, 0.9), 0.9)
	run_test("lerp_small", lerp(0.01, 0.02, 0.72), 0.0172)
	run_test("sign_positive", sign(13), 1)
	run_test("sign_negative", sign(-21), -1)
	run_test("sign_neutral", sign(0), 0)
	run_test("sign_negative_neutral", sign(-0), 0)
	run_test("smooth_step", smooth_step(0.2), 0.104)
	run_test("ping_pong", ping_pong(0.25), 0.25)
	run_test("ping_pong_over", ping_pong(1.25), 0.75)
	run_test("ping_pong_below", ping_pong(-0.25), 0.25)
	run_test("fract_positive", fract(8.5), 0.5)
	run_test("fract_negative", fract(-8.75), 0.25)
	run_test("wrap", wrap(12, 0, 10), 2.0)
	run_test("wrap_boundary", wrap(10, -10, 10), -10.0)
	run_test("wrap_negative", wrap(-12, -5, 5), -2.0)